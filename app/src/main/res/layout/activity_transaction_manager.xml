<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    tools:context=".TransactionManager">

    <TextView
        android:id="@+id/dateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textColor="@color/primaryColor"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDot"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/square_income"
        android:text="\/"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnDot2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnDot2" />

    <Button
        android:id="@+id/btnDot14"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="\*"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot"
        app:layout_constraintEnd_toEndOf="@+id/btnDot"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot" />

    <Button
        android:id="@+id/btnDot15"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="\-"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot14"
        app:layout_constraintEnd_toEndOf="@+id/btnDot"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot" />

    <Button
        android:id="@+id/btnDot16"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="\+"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot15"
        app:layout_constraintEnd_toEndOf="@+id/btnDot"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot" />

    <Button
        android:id="@+id/btnDot2"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="="
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/button19"
        app:layout_constraintEnd_toStartOf="@+id/btnDot"
        app:layout_constraintStart_toEndOf="@+id/btnDot3" />

    <Button
        android:id="@+id/btnDot7"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="9"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot2"
        app:layout_constraintEnd_toEndOf="@+id/btnDot2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot2" />

    <Button
        android:id="@+id/btnDot10"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="6"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot7"
        app:layout_constraintEnd_toEndOf="@+id/btnDot2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot2" />

    <Button
        android:id="@+id/btnDot13"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="3"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot10"
        app:layout_constraintEnd_toEndOf="@+id/btnDot2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot2" />

    <Button
        android:id="@+id/btnDot3"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/square_income"
        android:text="0"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnDot2"
        app:layout_constraintEnd_toStartOf="@+id/btnDot2"
        app:layout_constraintStart_toEndOf="@+id/btnDot4" />

    <Button
        android:id="@+id/btnDot6"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="8"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot3"
        app:layout_constraintEnd_toEndOf="@+id/btnDot3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot3" />

    <Button
        android:id="@+id/btnDot9"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="5"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot6"
        app:layout_constraintEnd_toEndOf="@+id/btnDot3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot3" />

    <Button
        android:id="@+id/btnDot12"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="2"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot9"
        app:layout_constraintEnd_toEndOf="@+id/btnDot3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot3" />

    <Button
        android:id="@+id/btnDot4"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/square_income"
        android:text="."
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnDot3"
        app:layout_constraintEnd_toStartOf="@+id/btnDot3"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnDot5"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="7"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot4"
        app:layout_constraintEnd_toEndOf="@+id/btnDot4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot4" />

    <Button
        android:id="@+id/btnDot8"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="4"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot5"
        app:layout_constraintEnd_toEndOf="@+id/btnDot4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot4" />

    <Button
        android:id="@+id/btnDot11"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="1"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDot8"
        app:layout_constraintEnd_toEndOf="@+id/btnDot4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnDot4" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:hint="Notatka"
        android:inputType="textPersonName"
        android:textColor="@android:color/background_light"
        android:textColorHighlight="@color/primaryColor"
        android:textColorHint="@android:color/background_light"
        android:textColorLink="@color/primaryColor"
        app:layout_constraintBottom_toTopOf="@+id/btnDot12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="50sp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@color/primaryColor"
        android:gravity="center"
        android:text="115,50 PLN"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/editText3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateTextView" />

    <Button
        android:id="@+id/button19"
        android:layout_width="0dp"
        android:layout_height="65sp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/square_income"
        android:text="Wybierz kategorię"
        android:textColor="@android:color/background_light"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>